{
  "extends": "airbnb",
  "rules": {
    "no-alert": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "arrow-parens": "off",
    "no-param-reassign": "off",
    "quotes": [2, "double"]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": [
    "html"
  ]
}
